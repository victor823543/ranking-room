{
  "name": "ranking-room",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:phone": "VITE_PHONE_MODE=true vite",
    "start": "vite --mode production",
    "build": "tsc --skipLibCheck && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:unit": "VITE_TESTING=true vitest vitest run tests/unit",
    "test:integration": "VITE_TESTING=true vitest vitest run tests/integration",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@hookform/resolvers": "^3.9.0",
    "@react-oauth/google": "^0.12.1",
    "@reduxjs/toolkit": "^2.0.1",
    "@tanstack/react-query": "^5.56.2",
    "@types/node": "^22.7.5",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.53.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.26.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "jsdom": "^23.2.0",
    "msw": "^2.6.0",
    "prettier": "^3.3.3",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  }
}
